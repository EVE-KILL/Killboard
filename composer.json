{
  "name": "eve-kill/esi-proxy",
  "description": "WebService that proxies calls to ESI",
  "type": "project",
  "license": "MIT",
  "authors": [
    {
      "name": "Michael Karbowiak",
      "email": "michael@karbowiak.dk"
    }
  ],
  "autoload": {
    "psr-4": {
      "EK\\": [
        "src/",
        "app/"
      ]
    }
  },
  "require": {
    "php": "^8.3",
    "ext-apcu": "*",
    "ext-mongodb": "*",
    "ext-redis": "*",
    "ext-pcntl": "*",
    "ext-simplexml": "*",
    "ext-bz2": "*",
    "monolog/monolog": "*",
    "league/container": "*",
    "mongodb/mongodb": "*",
    "slim/slim": "*",
    "slim/psr7": "*",
    "symfony/console": "*",
    "guzzlehttp/guzzle": "*",
    "kcs/class-finder": "*",
    "illuminate/collections": "*",
    "predis/predis": "*",
    "psy/psysh": "*",
    "poliander/cron": "*",
    "siriusphp/validation": "*",
    "league/csv": "*",
    "marcj/topsort": "*",
    "meilisearch/meilisearch-php": "*",
    "psr/log": "*",
    "tkhamez/eve-sso": "*",
    "symfony/rate-limiter": "*",
    "symfony/cache": "*",
    "symfony/lock": "*",
    "spiral/roadrunner-cli": "*",
    "nyholm/psr7": "*",
    "spiral/roadrunner-http": "*",
    "spiral/roadrunner": "*",
    "php-amqplib/php-amqplib": "*",
    "stomp-php/stomp-php": "*",
    "textalk/websocket": "*"
  },
  "require-dev": {
    "zeuxisoo/slim-whoops": "*",
    "symfony/var-dumper": "*",
    "roave/security-advisories": "dev-latest"
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true
    }
  }
}
